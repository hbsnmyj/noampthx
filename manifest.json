{
  "description": "Redirect cached AMP (accelerated mobile pages) results from Google.com to uncached ones for better compatibility.",
  "manifest_version": 2,
  "name": "Noampthx",
  "version": "0.1",
  "homepage_url": "https://github.com/hbsnmyj/noampthx",
  "application": {
      "gecko" : {
          "id": "noampthx@hoppinglife.com",
          "strict_min_version": "48.0"
      }
  },

  "permissions": [
    "*://*.google.com/*",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "incognito": "spanning",
  "background": {
    "scripts": [
        "scripts/noampthx.js"
    ]
  }
}
